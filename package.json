{
  "name": "virtual-file-system",
  "displayName": "Virtual File System",
  "description": "A VS Code extension that implements a FileSystemProvider with sample files",
  "version": "0.0.1",
  "publisher": "example-publisher",
  "author": {
    "name": "Example Developer"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/virtual-file-system.git"
  },
  "keywords": [
    "filesystem",
    "virtual",
    "provider",
    "sample",
    "development"
  ],
  "engines": {
    "vscode": "^1.108.0"
  },
  "enabledApiProposals": [
    "fileSearchProvider",
    "textSearchProvider"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vfs.mount",
        "title": "Mount Virtual File System",
        "category": "VFS"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "npx @vscode/vsce package",
    "publish": "npx @vscode/vsce publish",
    "package:pre-release": "npx @vscode/vsce package --pre-release",
    "publish:pre-release": "npx @vscode/vsce publish --pre-release"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.108.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^2.24.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  }
}